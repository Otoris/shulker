***REMOVED***
  "parserOptions": ***REMOVED***
    "ecmaVersion": 2020,
    "ecmaFeatures": ***REMOVED***
      "jsx": true
    ***REMOVED***,
    "sourceType": "module"
  ***REMOVED***,

  "plugins": [
    "react"
  ],

  "rules": ***REMOVED***
    "jsx-quotes": ["error", "prefer-single"],
    "react/jsx-boolean-value": "error",
    "react/jsx-closing-bracket-location": ["error", "tag-aligned"],
    "react/jsx-closing-tag-location": "error",
    "react/jsx-curly-brace-presence": ["error", ***REMOVED*** "props": "never", "children": "never" ***REMOVED***],
    "react/jsx-curly-newline": ["error", ***REMOVED*** "multiline": "consistent", "singleline": "consistent" ***REMOVED***],
    "react/jsx-curly-spacing": ["error", ***REMOVED***
      "attributes": ***REMOVED*** "when": "never" ***REMOVED***,
      "children": ***REMOVED*** "when": "never" ***REMOVED***,
      "allowMultiline": true
    ***REMOVED***],
    "react/jsx-equals-spacing": ["error", "never"],
    "react/jsx-first-prop-new-line": ["error", "multiline-multiprop"],
    "react/jsx-fragments": ["error", "syntax"],
    "react/jsx-handler-names": "error",
    "react/jsx-indent": ["error", 2, ***REMOVED*** "checkAttributes": true, "indentLogicalExpressions": true ***REMOVED***],
    "react/jsx-indent-props": ["error", 2],
    "react/jsx-key": "error",
    "react/jsx-no-comment-textnodes": "error",
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-no-target-blank": ["error", ***REMOVED*** "enforceDynamicLinks": "always" ***REMOVED***],
    "react/jsx-no-undef": "error",
    "react/jsx-pascal-case": ["error", ***REMOVED*** "allowAllCaps": false ***REMOVED***],
    "react/jsx-props-no-multi-spaces": "error",
    "react/jsx-tag-spacing": ["error", ***REMOVED***
      "closingSlash": "never",
      "beforeSelfClosing": "always",
      "afterOpening": "never",
      "beforeClosing": "never"
    ***REMOVED***],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/self-closing-comp": "error"
  ***REMOVED***
***REMOVED***
